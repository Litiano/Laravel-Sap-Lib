{
  "name": "litiano/sap",
  "version": "2.4.1",
  "description": "Lib para conectar com SAP via DI-API",
  "license": "MIT",
  "authors": [
    {
      "name": "Litiano Moura",
      "email": "litiano.moura@yahoo.com.br"
    }
  ],
  "require": {
    "php": ">=7.2.3",
    "laravel/framework": "^5.5|^6.0"
  },
  "suggest": {
    "ext-pdo_sqlsrv": "Necessário para conectar com SqlServer.",
    "ext-com_dotnet": "Necessário para conectar com SAP DI API."
  },
  "autoload": {
    "psr-4": {
      "Litiano\\Sap\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Litiano\\Sap\\SapServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
